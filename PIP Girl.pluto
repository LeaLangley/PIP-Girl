local function readFromFilePIP(the_path)
    local file = io.open(the_path, "r")
    if file then
        local webhook = file:read("*all")
        file:close()
        return webhook
    else
        return nil
    end
end

local function writeToFilePIP(payload, the_path)
    local file = io.open(the_path, "w")
    if file then
        file:write(payload)
        file:close()
    end
end

