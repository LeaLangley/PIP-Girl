local function find_in_table(tbl, value)
    for i, v in ipairs(tbl) do
        if v == value then
            return i
        end
    end
    return nil
end

local function contains(tbl, value)
    for ipairs(tbl) as v do
        if v == value then
            return true
        end
    end
    return false
end

local function getEntryByValue(tbl, value)
    for ipairs(tbl) as entry do
        if entry == value then
            return entry
        end
    end
end

